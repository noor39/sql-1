  FROM employees
  WHERE department_id = 50;
  
  DBMS_OUTPUT.PUT_LINE('Sum of Salaries: ' || v_sum_sal);
  DBMS_OUTPUT.PUT_LINE('Maximum Salary: ' || v_max_sal);
END;
