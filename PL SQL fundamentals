DECLARE 
v_outer_varaible VARCHAR2 (20):='GLOBAL VARIABLE';
BEGIN
    DECLARE
    v_inner_variable VARCHAR2(20):='LOCAL VARIABLE';
    BEGIN 
        DBMS_OUTPUT.PUT_LINE(v_inner_variable);
        DBMS_OUTPUT.PUT_LINE(v_outer_varaible);
    END;
            DBMS_OUTPUT.PUT_LINE(v_outer_varaible);
    END;
    /




