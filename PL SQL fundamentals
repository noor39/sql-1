  v_new_locn VARCHAR2(50):='Europe';
  BEGIN
    v_weight:=v_weight+1;
    v_new_locn:='Western'||v_new_locn; 
  END;
  v_weight:=v_weight+1;
  v_message:=v_message||'Is in stock!';
  v_new_locn:='Western'||v_new_locn;
END;
