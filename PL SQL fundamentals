v_max_dpt departments.department_id%TYPE;
BEGIN
SELECT max(department_id) INTO v_max_dpt
from employees;
  DBMS_OUTPUT.PUT_LINE( 'The max dept ID  is: '||v_max_dpt);

END;
/


