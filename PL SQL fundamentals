-- Subprogram with a BOOLEAN Parameter
CREATE OR REPLACE PROCEDURE p(x BOOLEAN) AUTHID
CURRENT_USER AS   
BEGIN

    IF x THEN   
        DBMS_OUTPUT.PUT_LINE('x is true');
    END IF;
END;
/
DECLARE
b BOOLEAN:=TRUE;
BEGIN
    p(b);
    END;
    /



